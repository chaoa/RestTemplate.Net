{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Error",
      "Microsoft": "Error"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true
    }
  },
  "poliy": {
    "MaxRetryCount": 3,
    "RetryIntervalMilliseconds": 100,
    "EnableCircuitBreaker": true,
    "AllowedCountBeforeBreaking": 5,
    "BreakingMilliseconds": 10000  ,
    "TimeOutMilliseconds": 1000
  }
}